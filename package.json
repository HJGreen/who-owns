{
  "name": "who-owns",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "isomorphic-unfetch": "^2.0.0",
    "netlify-lambda": "^1.6.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "3.4.1",
    "styled-components": "^3.2.6",
    "tailwindcss": "^1.4.6"
  },
  "scripts": {
    "build:tailwind": "tailwindcss build src/tailwind.css -o src/tailwind.generated.css",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind",
    "start": "react-scripts start",
    "build": "react-scripts build && yarn run lambda-build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lambda-serve": "netlify-lambda serve lambda-src",
    "lambda-build": "netlify-lambda build lambda-src",
    "postinstall": "netlify-lambda install"
  },
  "devDependencies": {
    "prettier": "^2.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
